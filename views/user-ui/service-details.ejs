    <!--==============================
    Service Area
    ==============================-->
    <section class="vs-service-wrapper space-top space-md-bottom">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="service-content row mb-30">
                        <div class="d-flex justify-content-center col-lg-5" data-wow-delay="0.3s">
                            <img src="/uploads/<%= service.image %>" alt="Service Image" class="w-100">
                        </div>
                        <div class="col-lg-7 mt-3 mt-lg-0">
                            <h2>
                                <%= service.name %>
                            </h2>
                            <p><%- service.description %></p>
                            <div class="d-flex gap-3 align-items-center">
                                <a href="tel:<%= siteSettings[0].number %>" class="text-decoration-none orange_border rounded-3 px-2 py-1">
                                    <i class="fa-solid fa-phone cursor-pointer"></i> Click to Call
                                </a>
                                <a href="https://wa.me/<%= siteSettings[0].number %>" class="text-decoration-none orange_border rounded-3 px-2 py-1">
                                    <i class="fa-brands fa-whatsapp cursor-pointer"></i> WhatsApp Chat
                                </a>
                            </div>
                        </div>

                     

                    </div>
                </div>
            </div>
        </div>
    </section>


    <section>
        <div class="container py-5">
            <h2 class="text-center mb-4">Related Products</h2>
            <div class="row">
                <% allServices.slice(0,4).forEach(service=> { %>
                    <div class="col-xl-3 col-lg-4 mb-25">
                        <div class="service-box ms-3 ms-lg-0">
                            <div class="sr-img">
                                <!-- Dynamically render service image -->
                                <img style="max-height: 270px;min-height: 270px;object-fit: contain;"
                                    src="/uploads/<%= service.image %>" alt="<%= service.name || 'Service Image' %>"
                                    class="w-100">
                            </div>
                            <div class="sr-content">
                                <!-- Dynamically render service name and link -->
                                <h3 class="h5">
                                    <a class="text-reset" href="/service-details/<%= service.id %>">
                                        <%= service.name %>
                                    </a>
                                </h3>
                                <a href="/service-details/<%= service.id %>" class="icon-btn style4">
                                    View More
                                </a>
                            </div>

                        </div>
                    </div>
                    <% }); %>


            </div>
        </div>
    </section>

