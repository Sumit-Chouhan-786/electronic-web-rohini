                        <div class="col-xl-3 col-lg-4 mb-25">
                            <div class="service-box ms-3 ms-lg-0">
                                <div class="sr-img">
                                    <!-- Dynamically render service image -->
                                    <img style="max-height: 270px;min-height: 270px;object-fit: contain;"
                                        src="/uploads/<%= service.image %>" alt="<%= service.name || 'Service Image' %>"
                                        class="w-100">
                                </div>
                                <div class="sr-content">
                                    <!-- Dynamically render service name and link -->
                                    <h3 class="h5 mt-2">
                                        <a class="text-reset" href="/service-details/<%= service.id %>">
                                            <%= service.name %>
                                        </a>
                                    </h3>
                                    <div class="sr-content">
                                        <a href="/service-details/<%= service.id %>" class="link-btn">
                                            View More<i class="fal fa-long-arrow-right"></i>
                                        </a>
                                    </div>
                                </div>
                              
                            </div>
                        </div>
                        <% }); %>
                </div>
                <!-- Custom Arrows -->
                <button class="custom_next_arrow custom-arrow">
                    <i class="fas fa-chevron-right"></i>
                </button>
                <button class="custom_prev_arrow custom-arrow">
                    <i class="fas fa-chevron-left"></i>
                </button>

            </div>
        </div>
    </section>
