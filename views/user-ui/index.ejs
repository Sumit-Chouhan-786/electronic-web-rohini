                        <h2 class="h1 ">Latest News</h2>
                        <p>Proactively revolutionize granular customer service after pandemic internal or "organic"
                            sources proactive human capital rather.</p>
                    </div>
                </div>
            </div>
            <div class="row vs-carousel wow fadeInUp" data-wow-delay="0.3s" data-slide-show="3" data-lg-slide-show="2">
                <% blogs.slice(0, 3).forEach(blog=> { %>
                    <div class="col-xl-4">
                        <div class="vs-blog blog-card">
                            <div class="blog-img">
                                <img style="max-height: 350px;min-height: 350px;object-fit: cover; background-position: top;"
                                    src="/uploads/<%= blog.blogImage %>" alt="Blog Image" class="w-100">
                                <div class="blog-date">
                                    <%= new Date(blog.createdAt).toLocaleDateString('en-US', { year: 'numeric' ,
                                        month: 'long' , day: 'numeric' }) %>
                                </div>
                            </div>
                            <div class="blog-content">
                                <h3 class="blog-title h5 font-body lh-base">
                                    <a href="/blog-details/<%= blog.id %>">
                                        <%= blog.heading %>
                                    </a>
                                </h3>
                                <a href="/blog-details/<%= blog.id %>" class="link-btn">Read More<i
                                        class="fa fa-arrow-alt-circle-right "></i></a>
                            </div>
                        </div>
                    </div>
                    <% }); %>

            </div>
        </div>
    </section>

    <%- include('../layout/ui-footer.ejs') %>