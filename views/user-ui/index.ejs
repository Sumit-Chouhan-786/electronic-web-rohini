<section class="vs-hero-wrapper position-relative">
    <div class="vs-hero-carousel hero_slider_layer position-relative" data-navprevnext="true" data-height="500"
        data-container="1900" data-slidertype="responsive">
        <% sliders.forEach(slider=> { %>
            <div class="ls-slide" data-ls="duration: 13000; transition2d: 5;">
                <img src="/uploads/<%= slider.sliderImage %>" alt="Hero Image" class="ls-bg">
                <h1 class="text-title ls-l ls-responsive position-relative z_100" data-ls-mobile="left: 80px; top: 80px; font-size: 80px;"
                    data-ls-tablet="left: 80px; top: 60px; font-size: 64px;"
                    data-ls-laptop="left: 80px; top: 80px; font-size: 56px;"
                    style="left: 150px; top: 150px; font-size: 48px; font-weight: 700;"
                    data-ls="delayin: 600; easingin: easeInOutSine; texttransitionin: true; textstartatin: transitioninstart; textdurationin: 2000; texttypein: words_asc; textshiftin: 200; textoffsetyin: -100; offsetyout: -100; durationout: 2000;">
                    <%= slider.title %>
                </h1>

                <p class="ls-l text-white ls-responsive ls-hide-sm text-dark position-relative z_100" data-ls-mobile="left: 80px;"
                    data-ls-tablet="left: 80px; top: 280px; font-size: 24px; width: 800px; line-height: 40px;"
                    data-ls-laptop="left: 80px; top: 300px; font-size: 20px; width: 600px; line-height: 36px;"
                    style="left: 150px; top: 280px; width: 480px; font-size: 14px; font-weight: 400; white-space: normal; letter-spacing: 0.02em; line-height: 24px;"
                    data-ls="delayin: 800; texttransitionin: true; textstartatin: transitioninstart; texttypein: lines_asc; textshiftin: 100; textoffsetyin: 100; textdurationin: 2000; offsetyout: 100; durationout: 2000;">
                    <%= slider.description %>
                </p>

                <div class="ls-btn ls-l ls-responsive position-relative z_100" data-ls-mobile="left: 80px;"
                    data-ls-tablet="left: 80px; top: 400px;" data-ls-laptop="left: 80px; top: 400px;"
                    style="left: 150px; top: 380px;"
                    data-ls="offsetyin: 200; durationin: 2000; delayin: 1400; offsetyout: 300; durationout: 2000;">
                    <a href="/contact" class="vs-btn style3 hover-white"
                        style="padding: 10px 20px; font-size: 14px;">View All Products<i class="fas fa-bolt"></i></a>
                </div>
            </div>
            <% }); %>
    </div>
</section>

